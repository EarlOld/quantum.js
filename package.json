{
  "name": "@earlold/quantum.js",
  "version": "0.5.0",
  "description": "Quantum.js is a library for quantum computing",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EarlOld/quantum.js.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "start": "tsx src/index.ts",
    "test": "jest",
    "build": "tsc",
    "prebuild:lib": "rm -rf lib-build",
    "build:lib": "tspc -p tsconfig.esm.json",
    "postbuild:lib": "cp package.json lib-build/ && cp README.md lib-build/"
  },
  "keywords": [
    "quantum",
    "quantum-circuit",
    "quantum-computing",
    "quantum-gates",
    "quantum-math",
    "quantum-mechanics",
    "quantum-physics",
    "quantum-programming",
    "quantum-qubits",
    "quantum-states",
    "quantum-superposition",
    "qubit",
    "QAOA"
  ],
  "author": "EarlOd<Dmytro Sapozhnyk>",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^27.0.0",
    "@types/quantum-circuit": "^0.9.0",
    "@types/three": "^0.182.0",
    "jest": "^29.7.0",
    "jsdom": "^27.3.0",
    "prettier": "^3.3.3",
    "sharp": "^0.33.4",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsx": "^4.16.2",
    "typescript": "^5.0.0",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "mathjs": "^13.0.3",
    "quantum-circuit": "^0.9.223",
    "three": "^0.170.0"
  }
}
